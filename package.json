{
  "name": "fl-assistant",
  "title": "Assistant",
  "version": "0.5.0",
  "description": "A plugin to help see what's happening in WordPress and navigate to posts and pages easily.",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaverbuilder/assistant.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:dist": "NODE_ENV=production webpack && node bin/build-dist.js",
    "build:stats": "webpack --env production --json > stats.json",
    "dev": "webpack --watch",
    "fix": "eslint src --fix",
    "fix:fluid": "eslint packages/@beaverbuilder/fluid/src --fix",
    "test": "jest && eslint src",
    "analyze": "NODE_ENV=analyze webpack --watch"
  },
  "license": "GPL-2.0+",
  "dependencies": {
    "@beaverbuilder/fluid": "^0.3.5",
    "axios": "0.18.1",
    "axios-cache-adapter": "^2.5.0",
    "camelcase": "^6.0.0",
    "framer-motion": "^1.10.3",
    "md5": "^2.2.1",
    "promise": "^8.1.0",
    "qs": "^6.9.3",
    "react-clipboard.js": "^2.0.16",
    "react-flip-toolkit": "^6.6.9",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@wordpress/babel-preset-default": "^3.0.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "expose-loader": "^0.7.5",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.13.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.10"
  }
}
